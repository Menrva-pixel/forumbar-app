{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-case-declarations":"off",
        "no-unused-vars":"off",
        "react/prop-types":"off",
        "react-in-jax-scope":"off"
    }
}
